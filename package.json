{
  "name": "webpackstarter",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Yaroslav",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "staging": "rimraf staging && webpack -p --config webpack.staging.js",
    "prod": "rimraf dist && webpack -p --config webpack.prod.js",
    "clean": "rimraf dist standing",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/**/*.js",
    "lint:css": "stylelint ./src/**/*.scss",
    "server:staging": "http-server staging -o",
    "server:prod": "http-server dist -o"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-critical-webpack-plugin": "^1.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.11.1",
    "jquery": "^3.1.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1"
  }
}
